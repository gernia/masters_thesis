	editor = {Koch, Lisa and Cardoso, M. Jorge and Ferrante, Enzo and Kamnitsas, Konstantinos and Islam, Mobarakol and Jiang, Meirui and Rieke, Nicola and Tsaftaris, Sotirios A. and Yang, Dong},
