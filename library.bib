	date = {2025-02-07},
	langid = {english},
	file = {Liang et al. - 2025 - Global trends and epidemiological impact of metabo.pdf:C\:\\Users\\Admin\\Zotero\\storage\\K9WLFKLM\\Liang et al. - 2025 - Global trends and epidemiological impact of metabo.pdf:application/pdf},
