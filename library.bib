	file = {IEEE Xplore Abstract Record:C\:\\Users\\Admin\\Zotero\\storage\\NWFWZIP6\\10480490.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\CHPDW93L\\Jo et al. - 2024 - Enhancing EEG Domain Generalization via Weighted C.pdf:application/pdf},
