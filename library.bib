	author = {Ballas, Aristotelis and Diou, Christos},
	date = {2024-02},
	keywords = {Electrocardiography, deep learning, Feature extraction, Adaptation models, Data models, domain generalization, Biological system modeling, 1D signal classification, Biosignal classification, Brain modeling, electrocardiogram ({ECG}) classification, electroencephalogram ({EEG}) classification, Electroencephalography},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Admin\\Zotero\\storage\\NK2Y6PS6\\10233054.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\6RB2KVQR\\Ballas und Diou - 2024 - Towards Domain Generalization for ECG and EEG Clas.pdf:application/pdf},
