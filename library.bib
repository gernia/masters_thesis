	file = {Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\RMYX82H6\\Khandelwal und Yushkevich - 2020 - Domain Generalizer A Few-Shot Meta Learning Frame.pdf:application/pdf},
