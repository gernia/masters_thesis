	date = {2011-04},
	keywords = {Electrocardiography, Rhythm, Artificial neural networks, Atrial fibrillation ({AF}), Classification algorithms, Databases, delta {RR} interval distribution difference curve ({dRDDC}), Histograms, Noise, {RR} interval},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Admin\\Zotero\\storage\\GY2BLMJZ\\5657251.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\DZ83SGFI\\Huang et al. - 2011 - A Novel Method for Detection of the Transition Bet.pdf:application/pdf},
