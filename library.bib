	author = {Ballas, Aristotelis and Diou, Christos},
	date = {2022-08},
	keywords = {Deep learning, Electrocardiography, deep learning, Hospitals, Databases, Training, Computational modeling, Biological system modeling, convolutional neural network, domain generalization biosignal classification, out of distribution},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Admin\\Zotero\\storage\\I7SX4XLZ\\9898255.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\KKB98VVR\\Ballas und Diou - 2022 - A Domain Generalization Approach for Out-Of-Distri.pdf:application/pdf},
