	file = {Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\A4U3RMRI\\Hammer et al. - 2021 - Automatic Classification of 2-, 3-, 4-, 6-, and 12.pdf:application/pdf},
}

