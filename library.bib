	author = {Han, Dong-Kyun and Jeong, Ji-Hoon},
	date = {2021-02},
	keywords = {Deep learning, deep learning, Classification algorithms, Task analysis, domain generalization, Brain modeling, Electroencephalography, Brain-computer interface, Brain-computer interfaces, electroen-cephalography, motor imagery, Risk management},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Admin\\Zotero\\storage\\MNZV6DEK\\9385322.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\A8CGRD3L\\Han und Jeong - 2021 - Domain Generalization for Session-Independent Brai.pdf:application/pdf},
