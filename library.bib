	number = {13},
	journaltitle = {Circulation},
	author = {Raghunath, Sushravya and Pfeifer, John M. and Ulloa-Cerna, Alvaro E. and Nemani, Arun and Carbonati, Tanner and Jing, Linyuan and {vanMaanen}, David P. and Hartzel, Dustin N. and Ruhl, Jeffery A. and Lagerman, Braxton F. and Rocha, Daniel B. and Stoudt, Nathan J. and Schneider, Gargi and Johnson, Kipp W. and Zimmerman, Noah and Leader, Joseph B. and Kirchner, H. Lester and Griessenauer, Christoph J. and Hafez, Ashraf and Good, Christopher W. and Fornwalt, Brandon K. and Haggerty, Christopher M.},
	date = {2021-03-30},
	keywords = {atrial fibrillation, deep learning, atrial flutter, neural network, prediction, stroke},
	file = {Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\QAMYWZPQ\\Raghunath et al. - 2021 - Deep Neural Networks Can Predict New-Onset Atrial .pdf:application/pdf},
