	date = {2023-11},
	keywords = {Arrhythmia, Deep learning, Humans, Electrocardiography, Deep Learning, Neural Networks, Computer, Signal Processing, Computer-Assisted, Attention module, Databases, Factual, Denoise and contrast attention ({DCAM}), Electrocardiogram ({ECG}), Premature ventricular contraction ({PVC}), Ventricular Premature Complexes},
	file = {Shin et al. - 2023 - Enhancing the performance of premature ventricular.pdf:C\:\\Users\\Admin\\Zotero\\storage\\2UXFJG49\\Shin et al. - 2023 - Enhancing the performance of premature ventricular.pdf:application/pdf},
