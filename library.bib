	date = {2020-10-01},
	keywords = {Deep learning, Atrial fibrillation, Electrocardiogram, Visualization, Black box, Morphology},
	file = {Baalman et al. - 2020 - A morphology based deep learning model for atrial .pdf:C\:\\Users\\Admin\\Zotero\\storage\\JKSSFX4H\\Baalman et al. - 2020 - A morphology based deep learning model for atrial .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Admin\\Zotero\\storage\\KD3XSQ6M\\S0167527320311530.html:text/html},
