	author = {Hammer, Alexander and Malberg, Hagen and Schmidt, Martin},
	date = {2023-10},
	keywords = {Atrial fibrillation, Rhythm, Databases, Couplings, Delay effects, Logistic regression, Stability analysis, von Alex, {ANS}},
