	keywords = {Domain Generalization, Diabetic Retinopathy, Mutual Information Regularization},
	file = {Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\LXNTCU2Z\\Matsun et al. - 2024 - DGM-DR Domain Generalization with Mutual Informat.pdf:application/pdf},
