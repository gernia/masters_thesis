	author = {Chiarugi, Franco and Varanini, Maurizio and Cantini, Federico and Conforti, Fabrizio and Vrouchos, Giorgos},
	date = {2007-08},
	keywords = {Electrocardiography, Atrial fibrillation, Biomedical signal processing, Computer science, {ECG} signal processing, Frequency, Hospitals, Noninvasive treatment, {QRS} cancelling, Rhythm, Signal processing algorithms, spectral analysis, Testing},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Admin\\Zotero\\storage\\RAHX8IZK\\4273601.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\H2SMVIBL\\Chiarugi et al. - 2007 - Noninvasive ECG as a Tool for Predicting Terminati.pdf:application/pdf},
