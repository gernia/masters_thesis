	author = {Dakshit, Sagnik and Prabhakaran, Balakrishnan},
	date = {2023-06},
	keywords = {Deep learning, Electrocardiography, Deep Learning, {ECG}, Lead, Heart, 12-lead, Decision support systems, {PTB}-{XL}, Stacked Generalization, Stacking, Telemedicine},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Admin\\Zotero\\storage\\N2A7MZN9\\10337156.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Admin\\Zotero\\storage\\DEZCWB3S\\Dakshit und Prabhakaran - 2023 - Twelve Lead Double Stacked Generalization for ECG .pdf:application/pdf},
